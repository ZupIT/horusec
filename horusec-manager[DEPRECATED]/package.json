{
  "name": "horusec-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --mode production --modern",
    "lint": "npm run lint-script && npm run lint-style",
    "lint-script": "vue-cli-service lint --no-fix",
    "lint-script-fix": "vue-cli-service lint --fix",
    "lint-style": "npx stylelint 'src/**/**/*.{css,sss,less,scss,sass}' --config .stylelintrc.json"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
    "moment": "^2.27.0",
    "randomcolor": "^0.5.4",
    "stylelint-webpack-plugin": "^2.1.0",
    "uuid": "^8.2.0",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-i18n": "^8.18.2",
    "vue-router": "^3.2.0",
    "vue-toast-notification": "^0.4.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "^4.4.6",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "vue-cli-plugin-vuetify": "^2.0.6",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
