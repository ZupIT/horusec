<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
    <Match>
        <Package name="~java\..*" />
    </Match>
    <Match>
        <Package name="~javax\..*" />
    </Match>
    <Match>
        <Package name="~org\.apache\..*" />
    </Match>
    <Match>
        <Package name="~org\.eclipse\..*" />
    </Match>
    <Match>
        <Package name="~org\.slf4j\..*" />
    </Match>
    <Match>
        <Package name="~org\.hibernate\..*" />
    </Match>
    <Match>
        <Package name="~org\.springframework\..*" />
    </Match>
    <Match>
        <Package name="~com\.mysql\..*" />
    </Match>
    <Match>
        <Package name="~spark\..*" />
    </Match>
    <Match>
        <Class name="~.*\.*Test" />
    </Match>
</FindBugsFilter>